<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Form -->
        <!-- Trash -->
        <record id="view_gateweb_invoice_trash_form" model="ir.ui.view">
            <field name="name">作廢發票</field>
            <field name="model">account.move</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="invoice_number"/>
                            <field name="cancel_reason"/>
                        </group>
                    </sheet>
                    <footer>
                        <button string="儲存" name="trash_invoice" type="object" class="btn-primary"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
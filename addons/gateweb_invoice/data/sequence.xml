<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_gateweb_invoice" model="ir.sequence">
            <field name="name">sequence gateweb invoice</field>
            <field name="code">gateweb.invoice</field>
            <field name="prefix">INV%(year)s%(month)s%(day)s%(hour)s</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>

        <record id="seq_gateweb_allowance" model="ir.sequence">
            <field name="name">sequence gateweb allowance</field>
            <field name="code">gateweb.allowance</field>
            <field name="prefix">ALW%(year)s%(month)s%(day)s%(hour)s</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>
    </data>
</odoo>